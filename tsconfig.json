{
  "compilerOptions": {
    "module": "esnext",
    "target": "es5",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "jsx": "preserve",
    "lib": ["esnext", "dom"]
  },
  "exclude": ["node_modules/*", "build/*"]
}
